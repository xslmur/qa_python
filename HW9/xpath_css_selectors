24. a[href='https://www.ncbi.nlm.nih.gov/nlmcatalog/journals/']
25. //div[@class='website-description']
26. //a[contains(text(), 'Advanced')]
27. //*[@id="search-form"]/div/a[1]/img
28. //input[@id='id_term']
29. //form[@id='search-form']
30. //div[@class='content-wrap']
31. //span[@class='usa-search-submit-text']
32. /html/head/meta[18]
